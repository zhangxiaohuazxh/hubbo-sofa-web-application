               xsi:schemaLocation="https://www.jooq.org/xsd/jooq-runtime-3.20.10.xsd">
    <jdbc>
        <driver>org.postgresql.Driver</driver>
        <url>
            jdbc:postgresql://hubbo.cn:34393/hubbo
        </url>
